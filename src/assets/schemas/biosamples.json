{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Filter",
  "type": "object",
  "properties": {
    "filter_cases": {
      "type": "string"
    },
    "stage": {
      "title": "Stage",
      "type": "array",
      "items": {
        "type": "string",
        "enum": ["Active", "Unassigned"]
      }
    },
    "priority": {
      "title": "Priority",
      "type": "array",
      "items": {
        "type": "string",
        "enum": ["low", "middle", "high"]
      }
    },
    "time": {
      "type": "object",
      "properties": {
        "start_date": { "type": "string" },
        "end_date": { "type": "string" }
      }
    },
    "notes": {
      "type": "string"
    },
    "queue": {
      "type": "string", 
      "enum": ["Yes", "No", "Maybe"]

    }

    
  }


}
